{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "name": "cds serve",
      "request": "launch",
      "type": "node",
      "cwd": "${workspaceFolder}",
      "runtimeExecutable": "cds",
      "args": [
        "serve",
        "--with-mocks",
        "--in-memory?"
      ],
      "skipFiles": [
        "<node_internals>/**"
      ]
    },
    {
      "name": "Run Dict.AdminApp",
      "type": "node",
      "request": "launch",
      "program": "${workspaceFolder}/Dict.AdminApp/node_modules/@sap/html5-repo-mock/index.js",
      "cwd": "${workspaceFolder}/Dict.AdminApp",
      "env": {
        "PORT": "6004",
        "MOCK_LOOKUP_DIRS": "[\"webapp\"]",
        "run.config": "{\"handlerId\":\"ui5_flat_run_config_handler_id\",\"runnableId\":\"/home/user/projects/Dict/app/Dict.AdminApp\"}"
      },
      "args": [
        "--standalone",
        "/DictAdminApp/index.html",
        "--ui5",
        "{\"version\":\".\"}"
      ],
      "envFile": "${workspaceFolder}/Dict.AdminApp/.env1",
      "preLaunchTask": "Build Dict.AdminApp"
    }
  ]
}